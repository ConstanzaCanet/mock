<section>
  <div class="container py-3" style="margin-top: 10%;">
      {{#each product}}
    <div class="card m-4">
      <div class="row ">
        <div class="col-md-4">
            <img src={{this.thumbnail}} alt={{this.id}} class="w-100" style="height: 100%; object-fit: cover;">
          </div>
          <div class="col-md-8 px-3">
            <div class="card-block px-3 m-4">
              <h4 class="card-title">{{this.name}}</h4>
              <p class="card-text">{{this.description}}</p>
              <p class="card-text"><b>Precio: ${{this.price}}</b></p>
              <h6 class='mb-0 pb-0'>Elige unidades</h6>
              <div class="btn-group mb-0 mt-0 pt-3" role="group" aria-label="Basic example" style="margin-top: 10%; width:100%">
                <button type="button" class="btn btn-secondary" id="menos">-</button>
                <button type="button" class="btn btn-secondary" disabled  id="counter-label"></button>
                <button type="button" class="btn btn-secondary" id="mas">+</button>
              </div>
              
              <div style="margin-top: 10%;">
                <button type="button" class="btn btn-warning">Comprar</button>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>
        {{/each}}
  </div>
  
</section>

<script>
  window.onload = function () {
    let mas = document.getElementById("mas");
    mas.onclick = incrementClick;

    let menos = document.getElementById("menos");
    menos.onclick = lessClick;
}

var counterVal = 0;

incrementClick = function() {
    updateDisplay(++counterVal);
}

function lessClick() {
  if(counterVal<=0){
    alert('cantidad no valida')
  }else{
    updateDisplay(--counterVal);
  }
}

function updateDisplay(val) {
    document.getElementById("counter-label").innerHTML = val;
}
</script>